{
  "name": "@react-preposition/docs",
  "version": "0.0.0",
  "description": "Create React layouts with predetermined positions.",
  "license": "MIT",
  "author": "Resist Design",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "module": "dist/react-preposition.esm.js",
  "size-limit": [
    {
      "path": "dist/react-preposition.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-preposition.esm.js",
      "limit": "10 KB"
    }
  ],
  "scripts": {
    "dev": "parcel ./src/index.html",
    "build:docs": "parcel build ./src/index.html --dist-dir ./docs/ --target web",
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "dependencies": {},
  "devDependencies": {
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@react-preposition/lib": "^1.0.1",
    "@resistdesign/sracl-mui-theme": "^1.1.0",
    "@size-limit/preset-small-lib": "^5.0.0",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-syntax-highlighter": "^13.5.1",
    "@types/styled-components": "^5.1.10",
    "@types/uuid": "^8.3.0",
    "flow-bin": "^0.120.1",
    "husky": "^6.0.0",
    "parcel": "^2.0.0-beta.2",
    "prettier": "^2.3.1",
    "raw-loader": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "react-syntax-highlighter": "^12.2.1",
    "size-limit": "^5.0.0",
    "styled-components": "^5.3.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/resistdesign/react-preposition.git"
  },
  "bugs": {
    "url": "https://github.com/resistdesign/react-preposition/issues"
  },
  "homepage": "https://github.com/resistdesign/react-preposition#readme",
  "keywords": [
    "React",
    "Layout",
    "Position"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 150,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "useTabs": false
  },
  "targets": {
    "web": {
      "context": "browser",
      "engines": {
        "browsers": [
          "last 2 version",
          "not dead",
          "> 0.2%"
        ]
      },
      "includeNodeModules": true,
      "outputFormat": "commonjs",
      "isLibrary": false,
      "scopeHoist": false
    }
  }
}
