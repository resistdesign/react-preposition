{
  "name": "@react-preposition/docs",
  "version": "0.0.0",
  "scripts": {
    "start": "parcel ./src/index.html",
    "build:docs": "parcel build ./src/index.html --dist-dir ../../docs/ --target web"
  },
  "dependencies": {},
  "devDependencies": {
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "react-preposition": "^1.0.1",
    "@resistdesign/sracl-mui-theme": "^1.1.0",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-syntax-highlighter": "^13.5.1",
    "@types/styled-components": "^5.1.10",
    "parcel": "^2.0.0-beta.2",
    "prettier": "^2.3.1",
    "raw-loader": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "react-syntax-highlighter": "^12.2.1",
    "styled-components": "^5.3.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.4"
  },
  "prettier": {
    "printWidth": 150,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "useTabs": false
  },
  "targets": {
    "web": {
      "context": "browser",
      "engines": {
        "browsers": [
          "last 2 version",
          "not dead",
          "> 0.2%"
        ]
      },
      "includeNodeModules": true,
      "outputFormat": "commonjs",
      "isLibrary": false,
      "scopeHoist": false
    }
  }
}
