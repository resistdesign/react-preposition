{
  "name": "react-preposition",
  "version": "2.0.0",
  "description": "Create React layouts with predetermined positions.",
  "license": "MIT",
  "author": "Resist Design",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 150,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/lib.esm.js",
  "size-limit": [
    {
      "path": "dist/lib.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/lib.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.1",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "husky": "^6.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^5.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "@types/uuid": "^8.3.0",
    "uuid": "^8.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/resistdesign/react-preposition.git"
  },
  "bugs": {
    "url": "https://github.com/resistdesign/react-preposition/issues"
  },
  "homepage": "https://github.com/resistdesign/react-preposition#readme",
  "keywords": [
    "React",
    "Layout",
    "Position"
  ]
}
